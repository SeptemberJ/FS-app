<view class="Detail"><view class="TopInfo"><text>{{"检验单号："+checkno}}</text><text>{{"检验日期："+dateTxt}}</text><text>{{"供应商："+supplier}}</text></view><view class="ListColumn"><text style="width:150rpx;">物料代码</text><text style="width:150rpx;">代码</text><text style="width:150rpx;">型号</text><text style="width:100rpx;">到货数量</text><text style="width:100rpx;">合格</text><text style="width:100rpx;">不合格</text></view><view class="ListBlock"><view class="ListMain"><block wx:if="{{listData.length==0}}"><text class="EmptyData">暂无数据</text></block><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="idx" wx:key="idx"><view style="background:aliceblue;margin-bottom:0rpx;"><view class="ListItem"><text style="width:150rpx;">{{item.matname}}</text><text style="width:150rpx;">{{item.matcode}}</text><text style="width:150rpx;">{{item.norms}}</text><text style="width:100rpx;">{{item.sendnum}}</text><input data-event-opts="{{[['blur',[['changeOkNum',[idx]]]],['input',[['__set_model',['$0','oknum','$event',[]],[[['listData','',idx]]]]]]]}}" class="uni-input" style="width:100rpx;border-bottom:0px solid #EEEEEE;" value="{{item.oknum}}" bindblur="__e" bindinput="__e"/><input data-event-opts="{{[['blur',[['changeFail',[idx]]]],['input',[['__set_model',['$0','failnum','$event',[]],[[['listData','',idx]]]]]]]}}" class="uni-input" style="width:100rpx;border-bottom:0px solid #EEEEEE;" value="{{item.failnum}}" bindblur="__e" bindinput="__e"/></view><view class="OperationBt"><text data-event-opts="{{[['tap',[['GoProject',['$0'],[[['listData','',idx]]]]]]]}}" style="background:#FA8888;" bindtap="__e">检验项目管理</text><text data-event-opts="{{[['tap',[['GoSerialNumber',['$0'],[[['listData','',idx]]]]]]]}}" style="background:#A4A2E4;" bindtap="__e">序列号管理</text></view></view></block></view></view><button class="SubmitBt" disabled="{{ifNoWork}}" data-event-opts="{{[['tap',[['save',['$event']]]]]}}" bindtap="__e">保 存</button></view>